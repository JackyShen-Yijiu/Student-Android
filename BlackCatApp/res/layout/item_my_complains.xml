<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:paddingTop="16dp"
    android:paddingBottom="16dp"
    android:layout_height="match_parent">
        <TextView 
            android:id="@+id/complain_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:text=""/>
        
        <TextView
            android:id="@+id/item_my_complain_name"
            style="@style/text_black_title"
            android:layout_toRightOf="@+id/complain_type"
            android:layout_alignBottom="@+id/complain_type"
            android:text="TextView" />

        <TextView
            android:id="@+id/item_my_complain_time"
            style="@style/text_gray_small"
            android:layout_alignParentRight="true"
            android:layout_marginRight="16dp"
            android:layout_alignBottom="@+id/complain_type"
            android:text="TextView" />

  		  <TextView
        android:id="@+id/item_my_complain_content"
        style="@style/text_gray_small"
        android:layout_below="@+id/complain_type"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="10dp"
        android:text="132123" />
  		  
  		  <ImageView 
  		      android:id="@+id/iv_one"
  		      android:layout_width="73dp"
  		      android:layout_below="@+id/item_my_complain_content"
  		      android:layout_marginTop="10dp"
  		      android:visibility="gone"
  		      android:layout_marginLeft="16dp"
  		      android:layout_height="73dp"/>
  		    <ImageView 
  		         android:id="@+id/iv_two"
  		      android:layout_width="73dp"
  		      android:layout_toRightOf="@+id/iv_one"
  		      android:layout_alignBottom="@+id/iv_one"
  		      android:layout_marginLeft="20dp"
  		      android:visibility="gone"
  		      android:layout_height="73dp"/>

</RelativeLayout>